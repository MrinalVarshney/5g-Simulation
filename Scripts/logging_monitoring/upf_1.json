    {
      "type": "stat",
      "title": "Total Active Sessions (All UPFs)",
      "id": 2,
      "gridPos": { "x": 0, "y": 1, "w": 6, "h": 3 },
      "datasource": "prometheus",
      "targets": [
        { "expr": "sum(fivegs_upffunction_upf_sessionnbr)", "refId": "A" }
      ],
      "options": { "reduceOptions": { "calcs": ["lastNotNull"] } }
    },
    {
      "type": "stat",
      "title": "Total PFCP Peers",
      "id": 3,
      "gridPos": { "x": 6, "y": 1, "w": 6, "h": 3 },
