  "refresh": "10s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": ["open5gs", "upf", "5g-core"],
  "templating": {
    "list": [
      {
        "name": "instance",
        "label": "UPF Instance",
        "type": "query",
        "datasource": "prometheus",
        "query": "label_values(instance)",
        "includeAll": true,
        "allValue": ".*",
        "refresh": 1
      }
    ]
  },
  "time": { "from": "now-2h", "to": "now" },
